   "source": [
    "#Define PCA\n",
    "pca = PCA(n_components=2)\n",
    "pca.fit(X_train)\n",
    "#Apply PCA on train and test set\n",
    "X_train_2d = pca.transform(X_train)\n",
    "X_test_2d = pca.transform(X_test)"
   ]
